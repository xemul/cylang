+) Args "1"
= n %~ Args.1
= rns (
	( 1000 "M" )
	( 900 "CM" )
	( 500 "D" )
	( 400 "CD" )
	( 100 "C" ) 
	( 90 "XC" ) 
	( 50 "L" ) 
	( 40 "XL" )
	( 10 "X" )
	( 9 "IX" )
	( 5 "V" )
	( 4 "IV" )
	( 1 "I" )
)
= s ""
~ {
	:- -. n
	~ rns {
		:+ -. ? >= n _.0 {
			= s + s _.1
			= n - n _.0
			:= _+
		} .
	}
}
` s
